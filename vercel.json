{
  "projects": [
    {
      "name": "frontend",
      "root": ".",
      "buildCommand": "npm run build",
      "outputDirectory": "dist",
      "framework": "vite"
    },
    {
      "name": "backend",
      "root": "backend",
      "buildCommand": "npm install",
      "outputDirectory": ".",
      "framework": null
    }
  ],
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/backend/server.js"
    }
  ]
}
